{
  "name": "Awesome Favicon",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Set a Font Awesome icon if there's no favicon",
  "web_accessible_resources": [
      "images/favicon/*.ico",
      "icons.txt"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "scripts/lib/jquery-2.1.4.min.js",
      "scripts/favicon.js"
    ]
  }],
  "icons": {
    "128": "images/fa-smile-o_128.png"
  }
}
